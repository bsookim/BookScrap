package com.scrap.response;

import com.scrap.entity.Books;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Getter;
import lombok.NoArgsConstructor;

@Getter
@NoArgsConstructor
@AllArgsConstructor
@Builder
public class BooksResponse {
	
	private Long id;
	
	private String title;
	
	private String price;
	
	private String stock;
	
	private String descpription;
	
	private String upc;
	
	private String productType;

	private String currnetUrl;
	
	
	public Books BooksResponse(Books bookBooks books) {
		this.id = books.getId();
		this.title = books.getTitle();
		this.price = books.getPrice();
		this.stock = books.getStock();
		this.descpription = books.getDescpription();
		this.upc = books.getUpc();
		this.productType = books.getProductType();
		this.currnetUrl = books.getCurrnetUrl();
		return books;
	}


	
}
